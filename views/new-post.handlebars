  <section class="card">
    <h2 class="page-title">Create New Post</h2>
    <form class="form add-form">
      <div class="form-group">
        <label for="post-title">Title</label>
        <input class="form-input" type="text" id="title"  />
      </div>
      <div class="form-group">
        <label for="post-content">Content</label><br>
        <textarea class="form-input" type="text" id="content" rows="5" cols="33"></textarea>
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">Create</button>
      </div>
    </form>

  </section>

  <script src="/js/newpost.js"></script>

